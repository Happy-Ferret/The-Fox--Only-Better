<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="chrome://thefoxonlybetter/skin/slimChrome.css" type="text/css"?>
<?xml-stylesheet href="chrome://thefoxonlybetter-os/skin/slimChrome.css" type="text/css"?>
<overlay id="slimChrome-overlay" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
	
	<toolbox id="navigator-toolbox" slimChrome="true">
		<box id="objName-slimChrome-slimmer" insertbefore="customToolbars"/>
		<hbox id="objName-slimChrome-container" insertafter="objName-slimChrome-slimmer" noPointerEvents="true">
			<vbox id="objName-slimChrome-toolbars" flex="1"/>
		</hbox>
	</toolbox>
	
</overlay>

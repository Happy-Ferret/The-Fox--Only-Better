<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="chrome://objPathString/skin/slimChrome.css" type="text/css"?>
<?xml-stylesheet href="chrome://objPathString-os/skin/slimChrome.css" type="text/css"?>
<overlay id="slimChrome-overlay"
	xmlns:svg="http://www.w3.org/2000/svg"
	xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
	
	<toolbox id="navigator-toolbox" slimChrome="true">
		<toolbar id="objName-slimChrome-slimmer"
			class="toolbar-primary"
			waitForSS="chrome://objPathString/skin/slimChrome.css chrome://objPathString-os/skin/slimChrome.css"
			fullscreentoolbar="true"
			context="toolbar-context-menu"
			insertbefore="customToolbars"
			customizable="false"
			ignoreCUI="true"/>
			
		<hbox id="objName-slimChrome-container"
			waitForSS="chrome://objPathString/skin/slimChrome.css chrome://objPathString-os/skin/slimChrome.css"
			insertafter="objName-slimChrome-slimmer"
			noPointerEvents="true">
			
			<box id="objName-slimChrome-toolbars-before" context="toolbar-context-menu">
				<svg:svg id="objName-slimChrome-svg-before"
					width="100%"
					height="100%"
					preserveAspectRatio="none slice"
					viewBox="0 0 1 1">
					
					<svg:path id="objName-slimChrome-svg-before-path" fill="transparent" d=""/>
				</svg:svg>
			</box>
			<vbox id="objName-slimChrome-toolbars" flex="1"/>
			<box id="objName-slimChrome-toolbars-after" context="toolbar-context-menu">
				<svg:svg id="objName-slimChrome-svg-after"
					width="100%"
					height="100%"
					preserveAspectRatio="none slice"
					viewBox="0 0 1 1">
					
					<svg:path id="objName-slimChrome-svg-after-path" fill="transparent" d=""/>
				</svg:svg>
			</box>
			<box id="objName-slimChrome-toolbars-bottom" context="toolbar-context-menu"/>
		</hbox>
	</toolbox>
	
	<vbox id="browser-panel">
		<svg:svg id="objName-slimChrome-clipPath" height="0">
			<svg:clipPath id="objName-slimChrome-clipPath-urlbar-wrapper" clipPathUnits="userSpaceOnUse">
				<svg:path id="objName-slimChrome-clipPath-urlbar-wrapper-path" d=""/>
			</svg:clipPath>
			<svg:clipPath id="objName-slimChrome-clipPath-toolbars-left" clipPathUnits="objectBoundingBox">
				<svg:path id="objName-slimChrome-clipPath-toolbars-left-path" d=""/>
			</svg:clipPath>
			<svg:clipPath id="objName-slimChrome-clipPath-toolbars-right" clipPathUnits="objectBoundingBox">
				<svg:path id="objName-slimChrome-clipPath-toolbars-right-path" d=""/>
			</svg:clipPath>
		</svg:svg>
	</vbox>
	
</overlay>
